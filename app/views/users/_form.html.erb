<%= form_for (@user) do |f| %>
<i class="material-icons prefix">email</i>
<%= f.label :email %>
<%= f.text_field :email%>
<i class="material-icons prefix">person_pin</i>
<%= f.label :username %>
<%= f.text_field :username %>
<i class="material-icons prefix">lock</i>
<%= f.label :password %>
<%= f.password_field :password%>
<%= f.submit %>
<% end %>
